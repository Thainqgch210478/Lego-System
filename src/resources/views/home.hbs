<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Slug</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <th scope="row">{{@index}}</th>
      <td>{{this.name}}</td>
      <td>{{this.quantity}}</td>
      <td>{{this.category.name}}</td>
      <td>{{this.theme.name}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>